{
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "restParams": true,
    "spread": true
  },
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "semi": [2, "always"],
    "object-curly-spacing": [2, "always"],
    "indent": [2,2, {"indentSwitchCase": true}],
    "no-multiple-empty-lines": [2, {"max": 1}],
    "spaced-comment": [2, "always"],
    "no-warning-comments": 2,
    "no-unneeded-ternary": 2,
    "no-debugger": 2,
    "no-console": 2,
    "no-empty": 2,
    "radix": 2,
    "brace-style": 2,
    "camelcase": 2,
    "max-depth": [2, 10],
    "max-statements": [2, 20],
    "complexity": [2, 10],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ]
}
